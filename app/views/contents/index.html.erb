<div class="container" id="container-cards">
  <h2> Facebook Images </h2>
  <%= render 'shared/card', publications: @facebook_images%>
  <h2> Facebook Posts </h2>
  <%= render 'shared/card', publications: @facebook_posts%>
  <h2> Facebook Likes </h2>
  <%= render 'shared/card', publications: @facebook_likes%>
</div>






<!-- Button trigger modal -->
<button type="button" class="btn-black-and-blue" data-toggle="modal" data-target="#exampleModal">
  Hide this data
</button>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Congrats</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        We've erased your selected elements
      </div>
      <div class="modal-footer">
        <button type="button" class="btn-white-and-blue" data-dismiss="modal">Close</button>
        <%= link_to "Go to your user dashboard", userstatistiques_path, class: "btn-white-and-blue" %>
      </div>
    </div>
  </div>
</div>
